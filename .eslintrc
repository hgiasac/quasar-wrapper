{
  "env": {
      "browser": true,
      "es6": true,
      "node": true
  },
  "parser": "@typescript-eslint/parser",

  "parserOptions": {
      "project": "./tsconfig.json",
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint",
      "functional"
  ],
  "settings": {
      "import/parsers": {
          "@typescript-eslint/parser": [".ts", ".tsx"]
      },
      "import/resolver": {
          "typescript": {
              "alwaysTryTypes": true
          }
      }
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "plugin:vue/vue3-strongly-recommended",
    "@vue/typescript/recommended",
    "prettier"
  ],
  "rules": {
  }
}
